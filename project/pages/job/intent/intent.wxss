page {
  background: #f2f2f2;
}
.wrap {
    display: flex;
    flex-flow: row wrap;
    justify-content: space-between;
    margin: 16rpx auto 0;
    width: 726rpx;
    background: #f2f2f2;
}
.list {
    margin-top: 16rpx;
    width: 350rpx;
    background: #ffffff;
    border-radius: 8rpx;
    box-shadow:0px 2px 8px #daedff;
}
.mainType {
    display: flex;
    flex-flow: row wrap;
    justify-content: space-between;
    align-items: center;
    padding: 0 40rpx;
    width: 100%;
    height: 88rpx;
    border-bottom: 1px solid #dbedff;
}
.mainType text {
    font-size: 28rpx;
}
image {
    width: 14rpx;
    height: 36rpx;
}
.childrenType {
    display: flex;
    flex-flow: row wrap;
    justify-content: flex-start;
    align-items: center;
    margin: 0 auto;
    width: 252rpx;
    height: 158rpx;
    font-size: 24rpx;
    color: #bbbbbb;
}
.childrenType navigator {
    width: 84rpx;
}